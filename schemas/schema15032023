CREATE KEYSPACE obk_mp WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TABLE obk_mp.creditcardaccountstransactions (
    creditcardaccountid text,
    transactionid text,
    amount decimal,
    billid text,
    billpostdate text,
    brandname text,
    brazilianamount decimal,
    chargeidentificator text,
    chargenumber int,
    creditdebittype text,
    currency text,
    feetype text,
    feetypeadditionalinfo text,
    generalid text,
    generatedat timestamp,
    identificationnumber text,
    linename text,
    obkaction text,
    othercreditsadditionalinfo text,
    othercreditstype text,
    payeemcc int,
    paymenttype text,
    transactionaladditionalinfo text,
    transactiondate text,
    transactionname text,
    transactiontype text,
    PRIMARY KEY (creditcardaccountid, transactionid)
) WITH CLUSTERING ORDER BY (transactionid ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''

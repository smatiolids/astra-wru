CREATE MATERIALIZED VIEW pe.mv_emprestimo_pedt008 AS
    SELECT penumcon, pecalpar, pecodofi, pecdgent, pecodent, pecodpro, pecodsub, penumper, peordpar
    FROM pe.pedt008
    WHERE penumcon IS NOT null AND pecalpar IS NOT null AND pecodofi IS NOT null AND pecdgent IS NOT null AND pecalpar IS NOT null AND pecodent IS NOT null AND pecodpro IS NOT null AND pecodsub IS NOT null AND penumper IS NOT null AND peordpar IS NOT null
    PRIMARY KEY ((penumcon, pecalpar, pecodofi, pecdgent, pecodent), pecodpro, pecodsub, penumper, peordpar)
    WITH CLUSTERING ORDER BY (pecodpro ASC, pecodsub ASC, penumper ASC, peordpar ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


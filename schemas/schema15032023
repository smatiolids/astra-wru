CREATE KEYSPACE gt WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TABLE gt.gtdtob3 (
    cod_entidad text,
    cod_centro text,
    num_contrato text,
    custom_id text,
    cd_cntr_ly text,
    cd_cntr_orig text,
    cd_deal text,
    cd_prod_deal text,
    cd_sist_orig text,
    cla_garantia text,
    cla_gardinam text,
    cod_divgara text,
    cod_divisa text,
    cod_entigara text,
    cod_estado text,
    cod_gar_bacen text,
    cod_garantia text,
    cod_producto text,
    cod_subprodu text,
    des_corta text,
    des_larga text,
    fec_actigara text,
    fec_altagara text,
    fec_altrelac text,
    fec_bajrelac text,
    fec_camlim text,
    fec_consgara text,
    fec_proceso text,
    fec_tipreg text,
    fec_vtogara text,
    filler1 text,
    filler2 text,
    imp_banco decimal,
    imp_cbiomgar decimal,
    imp_cbiomorg decimal,
    imp_cubiertl decimal,
    imp_cubierto decimal,
    imp_deudor decimal,
    imp_disponib decimal,
    imp_externo decimal,
    imp_limite decimal,
    imp_limitegl decimal,
    imp_limitego decimal,
    imp_limitevo decimal,
    incluido_ly text,
    ind_principa text,
    nr_deal decimal,
    num_bien int,
    num_garantia text,
    por_cubierto decimal,
    sdo_bloquead decimal,
    sdo_liquidad decimal,
    tip_cobertur text,
    tip_registro text,
    PRIMARY KEY ((cod_entidad, cod_centro, num_contrato), custom_id)
) WITH CLUSTERING ORDER BY (custom_id ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 93312000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


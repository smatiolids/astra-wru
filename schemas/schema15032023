    accountid text,
    customerid text,
    branchcode text,
    brandname text,
    checkdigit text,
    compecode text,
    currency text,
    customertype text,
    generatedat timestamp,
    number text,
    resourcename text,
    resourcestatus text,
    resourcetype text,
    subtype text,
    type text,
    PRIMARY KEY (accountid, customerid)
) WITH CLUSTERING ORDER BY (customerid ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';
CREATE INDEX accounts_by_customerid_v2 ON obk_bg.obk_accounts_v2 (customerid);

CREATE MATERIALIZED VIEW obk_bg.accounts_by_customerid_v2_index AS
    SELECT customerid, idx_token, accountid
    FROM obk_bg.obk_accounts_v2
    WHERE customerid IS NOT NULL
    PRIMARY KEY (customerid, idx_token, accountid)
    WITH CLUSTERING ORDER BY (idx_token ASC, accountid ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


CREATE KEYSPACE pix_spi WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TYPE pix_spi.pix_devolucao (
    cd_rast_devo text,
    cd_tran_envi text,
    vl_devo_pgto decimal,
    vl_pgto_intt decimal,
    vl_sald_devo decimal,
    dh_envi_pgto_intt text,
    dh_liqu_oper_bacen text,
    cd_situ_oper int,
    tx_comp_moti_devo text,
    tp_devo text
);


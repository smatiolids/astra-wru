    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE KEYSPACE scylla_manager WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TYPE scylla_manager.repair_unit (
    keyspace_name text,
    tables set<text>,
    coordinator_dc text
);

CREATE TYPE scylla_manager.schedule (
    start_date timestamp,
    interval_days int,
    num_retries int
);

CREATE TABLE scylla_manager.cluster (
    id uuid PRIMARY KEY,
    host text,
    name text,
    ssh_user text
) WITH bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE scylla_manager.scheduler_task (
    cluster_id uuid,
    type text,
    id uuid,
    enabled boolean,
    name text,
    properties blob,
    sched frozen<schedule>,
    tags set<text>,
    PRIMARY KEY (cluster_id, type, id)
) WITH CLUSTERING ORDER BY (type ASC, id ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE scylla_manager.gocqlx_migrate (
    name text PRIMARY KEY,
    checksum text,
    done int,
    end_time timestamp,
    start_time timestamp
) WITH bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE scylla_manager.scheduler_task_run (
    cluster_id uuid,
    type text,
    task_id uuid,
    id timeuuid,
    cause text,
    end_time timestamp,
    owner text,
    start_time timestamp,
    status text,
    PRIMARY KEY ((cluster_id, type, task_id), id)
) WITH CLUSTERING ORDER BY (id DESC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 360000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE scylla_manager.repair_run (
    cluster_id uuid,
    task_id uuid,
    id timeuuid,
    cause text,
    dc list<text>,
    end_time timestamp,
    host text,
    prev_id timeuuid,
    start_time timestamp,
    status text,
    token_ranges text,
    topology_hash uuid,
    units list<frozen<repair_unit>>,
    with_hosts list<text>,
    PRIMARY KEY ((cluster_id, task_id), id)
) WITH CLUSTERING ORDER BY (id DESC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 360000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE scylla_manager.repair_run_progress (
    cluster_id uuid,
    task_id uuid,
    run_id uuid,
    unit int,
    host text,
    shard int,
    last_command_id int,
    last_start_time timestamp,
    last_start_token bigint,
    segment_count int,
    segment_error int,
    segment_error_start_tokens list<bigint>,
    segment_success int,
    PRIMARY KEY ((cluster_id, task_id, run_id), unit, host, shard)
) WITH CLUSTERING ORDER BY (unit ASC, host ASC, shard ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 360000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


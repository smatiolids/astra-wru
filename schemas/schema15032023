CREATE KEYSPACE tc WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TABLE tc.tcdtgen (
    gen_tabla text,
    gen_entidad text,
    gen_idioma text,
    gen_clave text,
    gen_datos text,
    gen_timest_alt timestamp,
    gen_timest_mod timestamp,
    gen_usuario_alt text,
    gen_usuario_mod text,
    PRIMARY KEY ((gen_tabla, gen_entidad, gen_idioma, gen_clave))
) WITH bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 93312000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';
CREATE INDEX gen_tabla ON tc.tcdtgen (gen_tabla);

CREATE MATERIALIZED VIEW tc.gen_tabla_index AS
    SELECT gen_tabla, idx_token, gen_entidad, gen_idioma, gen_clave
    FROM tc.tcdtgen
    WHERE gen_tabla IS NOT NULL
    PRIMARY KEY (gen_tabla, idx_token, gen_entidad, gen_idioma, gen_clave)
    WITH CLUSTERING ORDER BY (idx_token ASC, gen_entidad ASC, gen_idioma ASC, gen_clave ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


CREATE KEYSPACE obk_mi WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TABLE obk_mi.imob_api_loans_entity (
    "contractNumber" text,
    "customerId" text,
    "CET" text,
    "amortizationScheduled" text,
    "amortizationScheduledAdditionalInfo" text,
    "brandName" text,
    "companyCnpj" text,
    "contractAmount" text,
    "contractDate" text,
    "contractId" text,
    "contractedFees" text,
    "contractedFinanceCharges" text,
    currency text,
    "disbursementDate" text,
    "dueDate" text,
    "firstInstalmentDueDate" text,
    "generatedAt" timestamp,
    "instalmentPeriodicity" text,
    "instalmentPeriodicityAdditionalInfo" text,
    "interestRates" text,
    "ipocCode" text,
    "obkAction" text,
    "productName" text,
    "productSubType" text,
    "productType" text,
    "resourceName" text,
    "resourceStatus" text,
    "resourceType" text,
    "settlementDate" text,
    PRIMARY KEY ("contractNumber", "customerId")
) WITH CLUSTERING ORDER BY ("customerId" ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 93312000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE obk_mi.imob_api_financings_payments (
    "contractNumber" text,
    "customerId" text,
    "brandName" text,
    "contractId" text,
    "generatedAt" timestamp,
    "obkAction" text,
    payments text,
    PRIMARY KEY ("contractNumber", "customerId")
) WITH CLUSTERING ORDER BY ("customerId" ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}

CREATE INDEX cuenta_index ON ug.ugdtmvr (cuenta);
CREATE INDEX oficina_index ON ug.ugdtmvr (oficina);

CREATE MATERIALIZED VIEW ug.oficina_index_index AS
    SELECT oficina, idx_token, cuenta, entidad, feliq, nio
    FROM ug.ugdtmvr
    WHERE oficina IS NOT NULL
    PRIMARY KEY (oficina, idx_token, cuenta, entidad, feliq, nio)
    WITH CLUSTERING ORDER BY (idx_token ASC, cuenta ASC, entidad ASC, feliq ASC, nio ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE MATERIALIZED VIEW ug.cuenta_index_index AS
    SELECT cuenta, idx_token, oficina, entidad, feliq, nio
    FROM ug.ugdtmvr
    WHERE cuenta IS NOT NULL
    PRIMARY KEY (cuenta, idx_token, oficina, entidad, feliq, nio)
    WITH CLUSTERING ORDER BY (idx_token ASC, oficina ASC, entidad ASC, feliq ASC, nio ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


CREATE TABLE obk_pe.fep (
    id text,
    penumper text,
    nr_sequ_doct text,
    nr_sequ_grup text,
    nr_sequ_pess_unic text,
    nr_sequ_rpre_grup text,
    generatedate timestamp,
    name text,
    number text,
    persontype text,
    socialname text,
    type text,
    PRIMARY KEY (id, penumper, nr_sequ_doct, nr_sequ_grup, nr_sequ_pess_unic, nr_sequ_rpre_grup)
) WITH CLUSTERING ORDER BY (penumper ASC, nr_sequ_doct ASC, nr_sequ_grup ASC, nr_sequ_pess_unic ASC, nr_sequ_rpre_grup ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';
CREATE INDEX nr_sequ_pess_unic_index ON obk_pe.fep (nr_sequ_pess_unic);
CREATE INDEX nr_sequ_rpre_grup_index ON obk_pe.fep (nr_sequ_rpre_grup);
CREATE INDEX nr_sequ_doct_index ON obk_pe.fep (nr_sequ_doct);
CREATE INDEX nr_sequ_grup_index ON obk_pe.fep (nr_sequ_grup);

CREATE MATERIALIZED VIEW obk_pe.nr_sequ_rpre_grup_index_index AS
    SELECT nr_sequ_rpre_grup, idx_token, id, penumper, nr_sequ_doct, nr_sequ_grup, nr_sequ_pess_unic
    FROM obk_pe.fep
    WHERE nr_sequ_rpre_grup IS NOT NULL
    PRIMARY KEY (nr_sequ_rpre_grup, idx_token, id, penumper, nr_sequ_doct, nr_sequ_grup, nr_sequ_pess_unic)
    WITH CLUSTERING ORDER BY (idx_token ASC, id ASC, penumper ASC, nr_sequ_doct ASC, nr_sequ_grup ASC, nr_sequ_pess_unic ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE MATERIALIZED VIEW obk_pe.nr_sequ_grup_index_index AS
    SELECT nr_sequ_grup, idx_token, id, penumper, nr_sequ_doct, nr_sequ_pess_unic, nr_sequ_rpre_grup
    FROM obk_pe.fep
    WHERE nr_sequ_grup IS NOT NULL
    PRIMARY KEY (nr_sequ_grup, idx_token, id, penumper, nr_sequ_doct, nr_sequ_pess_unic, nr_sequ_rpre_grup)
    WITH CLUSTERING ORDER BY (idx_token ASC, id ASC, penumper ASC, nr_sequ_doct ASC, nr_sequ_pess_unic ASC, nr_sequ_rpre_grup ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE MATERIALIZED VIEW obk_pe.nr_sequ_pess_unic_index_index AS
    SELECT nr_sequ_pess_unic, idx_token, id, penumper, nr_sequ_doct, nr_sequ_grup, nr_sequ_rpre_grup
    FROM obk_pe.fep
    WHERE nr_sequ_pess_unic IS NOT NULL
    PRIMARY KEY (nr_sequ_pess_unic, idx_token, id, penumper, nr_sequ_doct, nr_sequ_grup, nr_sequ_rpre_grup)
    WITH CLUSTERING ORDER BY (idx_token ASC, id ASC, penumper ASC, nr_sequ_doct ASC, nr_sequ_grup ASC, nr_sequ_rpre_grup ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE MATERIALIZED VIEW obk_pe.nr_sequ_doct_index_index AS
    SELECT nr_sequ_doct, idx_token, id, penumper, nr_sequ_grup, nr_sequ_pess_unic, nr_sequ_rpre_grup
    FROM obk_pe.fep
    WHERE nr_sequ_doct IS NOT NULL
    PRIMARY KEY (nr_sequ_doct, idx_token, id, penumper, nr_sequ_grup, nr_sequ_pess_unic, nr_sequ_rpre_grup)
    WITH CLUSTERING ORDER BY (idx_token ASC, id ASC, penumper ASC, nr_sequ_grup ASC, nr_sequ_pess_unic ASC, nr_sequ_rpre_grup ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy', 'space_amplification_goal': '1.5'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


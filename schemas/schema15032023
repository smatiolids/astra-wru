CREATE KEYSPACE stpes WITH replication = {'class': 'NetworkTopologyStrategy', 'norte': '3', 'sul': '3'}  AND durable_writes = true;

CREATE TABLE stpes.cadeia_societaria_online (
    penumper text PRIMARY KEY,
    faixas text,
    flag_1_nivel_cadeia text,
    flag_bf text,
    indicador_completude text,
    perc_completude decimal
) WITH bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 93312000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE stpes.cadeia_societaria (
    penumper text,
    penivel decimal,
    penumrel text,
    pecodrel text,
    petipper text,
    pehstamp timestamp,
    penumpri text,
    flag_1_nivel_cadeia text,
    flag_bf text,
    flag_completude text,
    origem text,
    pecaradm text,
    pecarcon text,
    pefecalt timestamp,
    pefecini timestamp,
    pefecter timestamp,
    peindrep text,
    penatjur text,
    penomper text,
    penomrel text,
    penumero text,
    peporcom decimal,
    peporpar decimal,
    peporpri decimal,
    peporrel decimal,
    petipdoc text,
    petippar text,
    peusualt text,
    peusumod text,
    porreltot decimal,
    relnumero text,
    reltipdoc text,
    PRIMARY KEY ((penumper, penivel, penumrel, pecodrel, petipper, pehstamp), penumpri)
) WITH CLUSTERING ORDER BY (penumpri ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 93312000
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';
CREATE INDEX idx_penumpri ON stpes.cadeia_societaria (penumpri);

CREATE MATERIALIZED VIEW stpes.idx_penumpri_index AS
    SELECT penumpri, idx_token, penumper, penivel, penumrel, pecodrel, petipper, pehstamp
    FROM stpes.cadeia_societaria
    WHERE penumpri IS NOT NULL
    PRIMARY KEY (penumpri, idx_token, penumper, penivel, penumrel, pecodrel, petipper, pehstamp)
    WITH CLUSTERING ORDER BY (idx_token ASC, penumper ASC, penivel ASC, penumrel ASC, pecodrel ASC, petipper ASC, pehstamp ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''

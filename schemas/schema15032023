CREATE INDEX idx_cd_rast_pgto ON pix_spi.pix_cobranca (cd_rast_pgto);
CREATE INDEX idx_cd_tran_recb ON pix_spi.pix_cobranca (cd_tran_recb);
CREATE INDEX idx_nr_cpf_cnpj_emis ON pix_spi.pix_cobranca (nr_cpf_cnpj_emis);

CREATE MATERIALIZED VIEW pix_spi.idx_nr_cpf_cnpj_emis_index AS
    SELECT nr_cpf_cnpj_emis, idx_token, nr_cpf_cnpj_recd, cd_rast_pgto
    FROM pix_spi.pix_cobranca
    WHERE nr_cpf_cnpj_emis IS NOT NULL
    PRIMARY KEY (nr_cpf_cnpj_emis, idx_token, nr_cpf_cnpj_recd, cd_rast_pgto)
    WITH CLUSTERING ORDER BY (idx_token ASC, nr_cpf_cnpj_recd ASC, cd_rast_pgto ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE MATERIALIZED VIEW pix_spi.pix_cobranca_by_emis AS
    SELECT *
    FROM pix_spi.pix_cobranca
    WHERE nr_cpf_cnpj_emis IS NOT null AND cd_rast_pgto IS NOT null AND nr_cpf_cnpj_recd IS NOT null
    PRIMARY KEY (nr_cpf_cnpj_emis, cd_rast_pgto, nr_cpf_cnpj_recd)
    WITH CLUSTERING ORDER BY (cd_rast_pgto ASC, nr_cpf_cnpj_recd ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = {'keys': 'ALL', 'rows_per_partition': 'ALL'}
    AND comment = ''
    AND compaction = {'class': 'IncrementalCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND crc_check_chance = 1.0
    AND dclocal_read_repair_chance = 0.0
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';


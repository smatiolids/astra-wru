CREATE KEYSPACE dsefs WITH REPLICATION = { 'class' : 'org.apache.cassandra.locator.SimpleStrategy', 'replication_factor': '1' } AND DURABLE_WRITES = true;

CREATE TYPE dsefs.ace (
    action text,
    principal text,
    is_group boolean,
    inherit_options int,
    permissions int
);

CREATE TYPE dsefs.chunk (
    chunk_id int,
    length bigint
);


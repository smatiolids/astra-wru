CREATE TABLE em4cas001.categoria (
    id uuid,
    dataedicao timestamp,
    idpai uuid,
    datacriacao timestamp,
    desambiguacao boolean,
    descricao text,
    idagente uuid,
    nome text,
    solr_query text,
    usuariocriacao text,
    usuarioedicao text,

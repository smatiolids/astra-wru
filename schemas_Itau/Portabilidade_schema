    num_cont text,
    num_cpf_clie text,
    num_dv_cont text,
    num_simu_limi_unic bigint,
    num_titr_cont int,
    num_unid_ated_cogl text,
    pct_iof_cesp decimal,
    pct_juro_moro_mnsl_cesp decimal,
    pct_limi_vlr_totl_oper decimal,
    pct_txa_cet_anua_ctrt_cesp decimal,
    pct_txa_cet_mnsl_ctrt_cesp decimal,
    pct_txa_juro_anua_cesp decimal,
    pct_txa_juro_mnsl_cesp decimal,
    pct_vlr_totl_oper decimal,
    sig_cana_dtbc text,
    vlr_iof_oper decimal,
    vlr_limi_adic_cesp double,
    vlr_limi_cont double,
    vlr_limi_ctrt_prod_rott double,
    vlr_suge_limi_cred_papr double,
    vlr_totl_oper_ctrt_cesp double,
    PRIMARY KEY (cod_efet_cesp, dat_hor_cria_efet_cesp)
) WITH CLUSTERING ORDER BY (dat_hor_cria_efet_cesp DESC)
    AND read_repair_chance = 0.0
    AND dclocal_read_repair_chance = 0.1
    AND gc_grace_seconds = 864000
    AND bloom_filter_fp_chance = 0.01
    AND caching = { 'keys' : 'ALL', 'rows_per_partition' : 'NONE' }
    AND comment = ''
    AND compaction = { 'class' : 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy', 'max_threshold' : 32, 'min_threshold' : 4 }
    AND compression = { 'chunk_length_in_kb' : 64, 'class' : 'org.apache.cassandra.io.compress.LZ4Compressor' }
    AND default_time_to_live = 0
    AND speculative_retry = '99PERCENTILE'
    AND min_index_interval = 128
    AND max_index_interval = 2048
    AND crc_check_chance = 1.0
    AND cdc = false
    AND memtable_flush_period_in_ms = 0;


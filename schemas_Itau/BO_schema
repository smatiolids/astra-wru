    min bigint,
    p50 bigint,
    p75 bigint,
    p90 bigint,
    p95 bigint,
    p98 bigint,
    p99 bigint,
    PRIMARY KEY ((node_ip, keyspace_name, table_name), histogram_id)
) WITH CLUSTERING ORDER BY (histogram_id DESC)
    AND read_repair_chance = 0.0
    AND dclocal_read_repair_chance = 0.0
    AND gc_grace_seconds = 60
    AND bloom_filter_fp_chance = 0.01
    AND caching = { 'keys' : 'NONE', 'rows_per_partition' : 'NONE' }
    AND comment = ''
    AND compaction = { 'class' : 'org.apache.cassandra.db.compaction.TimeWindowCompactionStrategy', 'max_threshold' : 32, 'min_threshold' : 4 }
    AND compression = { 'chunk_length_in_kb' : 64, 'class' : 'org.apache.cassandra.io.compress.LZ4Compressor' }
    AND default_time_to_live = 0
    AND speculative_retry = '99PERCENTILE'
    AND min_index_interval = 128
    AND max_index_interval = 2048
    AND crc_check_chance = 1.0
    AND cdc = false
    AND memtable_flush_period_in_ms = 3600000
    AND nodesync = { 'enabled' : 'false' };

CREATE TABLE dse_perf.dropped_messages (
    node_ip inet,
    histogram_id timestamp,
    verb text,
    global_15min_rate double,
    global_1min_rate double,
    global_5min_rate double,
    global_count bigint,
    global_mean_rate double,
    internal_15min_rate double,
    internal_1min_rate double,
    internal_5min_rate double,
    internal_count bigint,
    internal_latency_max double,
    internal_latency_mean double,
    internal_latency_median double,
    internal_latency_min double,
    internal_latency_p75 double,
    internal_latency_p90 double,
    internal_latency_p95 double,
    internal_latency_p98 double,
    internal_latency_p99 double,
    internal_latency_stdev double,
    internal_mean_rate double,
    xnode_15min_rate double,
    xnode_1min_rate double,
    xnode_5min_rate double,
    xnode_count bigint,
    xnode_max double,
    xnode_mean double,
    xnode_mean_rate double,
    xnode_median double,
    xnode_min double,
    xnode_p75 double,
    xnode_p90 double,
    xnode_p95 double,
    xnode_p98 double,
    xnode_p99 double,
    xnode_stdev double,
    PRIMARY KEY (node_ip, histogram_id, verb)
) WITH CLUSTERING ORDER BY (histogram_id DESC, verb ASC)
    AND read_repair_chance = 0.0
    AND dclocal_read_repair_chance = 0.0
    AND gc_grace_seconds = 60
    AND bloom_filter_fp_chance = 0.01
    AND caching = { 'keys' : 'NONE', 'rows_per_partition' : 'NONE' }
    AND comment = ''
    AND compaction = { 'class' : 'org.apache.cassandra.db.compaction.TimeWindowCompactionStrategy', 'max_threshold' : 32, 'min_threshold' : 4 }
    AND compression = { 'chunk_length_in_kb' : 64, 'class' : 'org.apache.cassandra.io.compress.LZ4Compressor' }
    AND default_time_to_live = 0
    AND speculative_retry = '99PERCENTILE'
    AND min_index_interval = 128
    AND max_index_interval = 2048
    AND crc_check_chance = 1.0
    AND cdc = false
    AND memtable_flush_period_in_ms = 3600000
    AND nodesync = { 'enabled' : 'false' };


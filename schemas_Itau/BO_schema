CREATE KEYSPACE hwcas001 WITH REPLICATION = { 'class' : 'org.apache.cassandra.locator.NetworkTopologyStrategy', 'novCTMM1': '3', 'novCTMM2': '3' } AND DURABLE_WRITES = true;

CREATE TYPE hwcas001.ctud_ende_pjur (
    nom_logr_ende text,
    num_logr_ende text,
    nom_bair_logr text,
    nom_cida_ende text,
    cod_cep_ende text,
    cod_uf_ende text,
    txt_ctud_ende_pjur text
);

CREATE TYPE hwcas001.ctud_ocup_scna_pjur (
    desc_ocup_secu text,
    desc_cnae_secu text
);

CREATE TYPE hwcas001.ctud_cert_pjur (
    num_cpf bigint,
    num_cnpj bigint,
    nom_raza_soci_empr text,
    dat_abet_empr date,
    num_rgto_empr bigint,
    vlr_capi_soci decimal,
    des_cnae_prin text,
    dat_atui_cslt_forn_exto timestamp,
    dat_inio_atii_pjur date,
    des_ocup_prin text,
    num_reci_cobr_cslt text,
    num_rg_pfis text,
    num_idef bigint,
    nom_micr_emps_indl text,
    sig_orgo_emio text,
    sig_uf_emio text,
    nom_situ_cada_vign text,
    des_form_atua_pjur text,
    txt_ctud_ende_pjur set<frozen<hwcas001.ctud_ende_pjur>>,
    nom_fant text,
    des_ocup_secu_pjur set<frozen<hwcas001.ctud_ocup_scna_pjur>>
);

CREATE TABLE hwcas001.tbhw001_cslt_ende_pjur_brau (
    num_cnpj bigint,
    num_seqe_ende int,
    cod_caix_pstl text,
    cod_cep_ende text,
    cod_efte_info text,
    cod_situ_cida text,
    cod_situ_cnpj text,
    cod_situ_ende text,
    cod_situ_uf_ende text,
    cod_tipo_loci_ende text,
    cod_titr_docm text,
    cod_uf_ende text,
    dat_psst timestamp,
    dat_reto_efte_info date,
    dat_rfrc_info date,
    ind_orig_info_inrn text,
    nom_bair_logr text,
    nom_cida_ende text,
    nom_cmpl_ende text,
    nom_logr_ende text,
    num_clas_rcna int,
    num_logr_ende text,
    num_prio_orig int,
    num_prio_orig_recn int,
    num_qtd_pess_asdo_ende bigint,
    qtd_orig int,
    qtd_orig_recn bigint,
    vlr_novo_rcna int,
    PRIMARY KEY (num_cnpj, num_seqe_ende)
) WITH CLUSTERING ORDER BY (num_seqe_ende ASC)
    AND read_repair_chance = 0.0
    AND dclocal_read_repair_chance = 0.1
    AND gc_grace_seconds = 864000
    AND bloom_filter_fp_chance = 0.01
    AND caching = { 'keys' : 'ALL', 'rows_per_partition' : 'NONE' }
    AND comment = ''
    AND compaction = { 'class' : 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy', 'max_threshold' : 32, 'min_threshold' : 4 }
    AND compression = { 'chunk_length_in_kb' : 64, 'class' : 'org.apache.cassandra.io.compress.LZ4Compressor' }
    AND default_time_to_live = 0
    AND speculative_retry = '99PERCENTILE'
    AND min_index_interval = 128
    AND max_index_interval = 2048
    AND crc_check_chance = 1.0
    AND cdc = false
    AND memtable_flush_period_in_ms = 0
    AND nodesync = { 'enabled' : 'true' };

CREATE TABLE hwcas001.tbhw002_cslt_emai_pjur (
    num_cnpj bigint,
    num_seqe_ctto_elet text,
    cod_efte_info text,
    cod_situ_cnpj text,
    cod_situ_ctto_elet text,
    cod_situ_reto_emai text,
    cod_tipo_ctto_elet text,
    cod_tipo_loci_ctto_elet text,
    cod_titr_docm text,
    dat_psst timestamp,
    dat_reto_ctto_emai date,

CREATE KEYSPACE system_auth WITH REPLICATION = { 'class' : 'org.apache.cassandra.locator.NetworkTopologyStrategy', 'novCTMM1': '3', 'novCTMM2': '3' } AND DURABLE_WRITES = true;


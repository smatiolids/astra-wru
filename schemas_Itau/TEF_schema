    internal_1min_rate double,
    internal_5min_rate double,
    internal_count bigint,
    internal_latency_max double,
    internal_latency_mean double,
    internal_latency_median double,
    internal_latency_min double,
    internal_latency_p75 double,
    internal_latency_p90 double,
    internal_latency_p95 double,
    internal_latency_p98 double,
    internal_latency_p99 double,
    internal_latency_stdev double,
    internal_mean_rate double,

    xnode_1min_rate double,
    xnode_5min_rate double,
    xnode_count bigint,
    xnode_max double,
    xnode_mean double,
    xnode_mean_rate double,
    xnode_median double,
    xnode_min double,
    xnode_p75 double,
    xnode_p90 double,
    xnode_p95 double,
    xnode_p98 double,
    xnode_p99 double,
    xnode_stdev double,
